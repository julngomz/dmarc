      "source-layer": "transportation",
      "filter": [
        "==",
        "class",
        "tertiary"
      ],
      "layout": {
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(77, 166, 238, 1)",
        "line-opacity": 0.5,
        "line-width": {
          "stops": [
            [
              11,
